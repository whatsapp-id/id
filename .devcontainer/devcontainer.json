{
  "name": "Auto Run Server.js",
  "image": "mcr.microsoft.com/devcontainers/node:20",
  "postCreateCommand": "npm install",
  "postStartCommand": "node server.js",
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "Bot Manager",
      "onAutoForward": "openBrowser"
    }
  }
}
